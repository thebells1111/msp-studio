{
	"name": "msp-studio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"vite dev --host --port 3333\" \"cross-env DEV=true nodemon server/index.js\"",
		"vite": "vite dev --host --port 3333",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"start": "cross-env DEV=true nodemon server/index.js",
		"docker:inspect": "docker run -it --entrypoint /bin/bash msp",
		"docker:build": "docker build -t msp .",
		"docker:start": "docker run -it -e CREDENTIALS_PATH=/usr/src/app/msp/credentials/credentials.json -v /var/docker/msp/credentials:/usr/src/app/msp/credentials -v /var/docker/msp/mspDB:/usr/src/app/msp/mspDB -v /var/docker/msp/albums:/usr/src/app/msp/public -p 3000:8000 msp",
		"docker:all": "npm run docker:build && npm run docker:start"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/adapter-vercel": "^2.0.4",
		"@sveltejs/kit": "^1.20.4",
		"archiver": "^6.0.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"fast-xml-parser": "^3.20.3",
		"file-saver": "^2.0.5",
		"form-data": "^4.0.0",
		"html-entities": "^2.3.3",
		"just-clone": "^6.2.0",
		"level": "^8.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^4.0.0",
		"svelte-copy": "^1.4.1",
		"svelte-select": "^5.5.2",
		"uuid": "^9.0.0",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"express-rate-limit": "^7.0.2",
		"express-session": "^1.17.3"
	}
}
